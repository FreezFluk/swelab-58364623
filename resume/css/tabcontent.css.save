.tablink {
    background-color: #c4c4c4;
    color: black;
    float: left;
    border: none;
    outline: none;
    cursor: pointer;
    padding: 14px 16px;
    font-size: 17px;
    font-weight: bold;
    width: 25%;
}

.tablink:hover {
    background-color: white;
}

.tabcontent {
    color: black;
    display: none;
    padding: 100px 20px;
    height: 100%;
}

#EDUCATIONS {background-color: #01ABAA;}
#PICTURES {background-color: #FEDC3D;}
#SKILLS {background-color: #FEA680
;}
#REFERENCES {background-color: rgb(36, 173, 24);}
